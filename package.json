{
  "name": "protontasks",
  "version": "2.0.0",
  "description": "TypeScript demo to do list",
  "author": {
    "name": "Alberto Basalo",
    "email": "learn@bitademy.com",
    "url": "https://twitter.com/bit_ademy"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomicBuilders/protontasks/issues"
  },
  "homepage": "https://github.com/AtomicBuilders/protontasks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtomicBuilders/protontasks.git"
  },
  "keywords": [
    "JS",
    "TypeScript",
    "Prettier",
    "Jest",
    "Seed",
    "Template"
  ],
  "main": "main.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "rm -rf dist && parcel build ./src/index.html",
    "predeploy": "parcel build ./src/index.html --public-url /ProtonTasks",
    "deploy": "gh-pages -d dist",
    "test": "jest --watch -o",
    "format": "prettier --write \"./**/*.{js,ts,json}\"",
    "lint": "eslint . --ext .ts",
    "jest": "jest",
    "prerelease": "standard-version ",
    "release": "git push --follow-tags origin master",
    "updates": "ncu -u"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-jest": "^26.6.3",
    "cssnano": "^5.0.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "standard-version": "^9.3.0",
    "npm-check-updates": "^11.5.10"
  }
}
