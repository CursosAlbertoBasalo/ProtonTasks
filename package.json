{
  "name": "proton",
  "version": "1.0.0",
  "description": "Profesional TypeScript project setup",
  "author": {
    "name": "Alberto Basalo",
    "email": "learn@bitademy.com",
    "url": "https://twitter.com/bit_ademy"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomicBuilders/proton/issues"
  },
  "homepage": "https://github.com/AtomicBuilders/proton#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtomicBuilders/proton.git"
  },
  "keywords": [
    "JS",
    "TypeScript",
    "Prettier",
    "Jest",
    "Seed",
    "Template"
  ],
  "main": "main.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "rm -rf dist && parcel build ./src/index.html",
    "predeploy": "rm -rf dist && parcel build ./src/index.html --public-url /proton",
    "deploy": "gh-pages -d dist",
    "test": "jest --watch -o",
    "format": "prettier --write \"./**/*.{js,ts,json}\"",
    "lint": "eslint . --ext .ts",
    "jest": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  }
}
