{
  "name": "protontasks",
  "version": "1.0.0",
  "description": "TypeScript demo to do list",
  "author": {
    "name": "Alberto Basalo",
    "email": "learn@bitademy.com",
    "url": "https://twitter.com/bit_ademy"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomicBuilders/protontasks/issues"
  },
  "homepage": "https://github.com/AtomicBuilders/protontasks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtomicBuilders/protontasks.git"
  },
  "keywords": [
    "JS",
    "TypeScript",
    "Prettier",
    "Jest",
    "Seed",
    "Template"
  ],
  "main": "main.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "rm -rf dist && parcel build ./src/index.html",
    "predeploy": "rm -rf dist && parcel build ./src/index.html --public-url /ProtonTasks",
    "deploy": "gh-pages -d dist",
    "test": "jest --watch -o",
    "format": "prettier --write \"./**/*.{js,ts,json}\"",
    "lint": "eslint . --ext .ts",
    "jest": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "babel-jest": "^26.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gh-pages": "^2.2.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "typescript": "^3.8.3"
  }
}
